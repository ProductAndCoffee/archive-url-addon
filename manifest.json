{
  "manifest_version": 2,
  "name": "Archive.org URL Checker",
  "version": "1.0",
  "description": "Checks if the current page is archived on archive.org.",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Check Archive.org"
  },
  "icons": {
    "48": "icon.png"
  }
}